{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "no-param-reassign": "off"
  },
  "plugins": ["prettier"]
}
